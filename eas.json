{
  "cli": { "version": ">= 16.32.0", "appVersionSource": "remote" },
  "build": {
    "development": { "developmentClient": true, "distribution": "internal" },
    "preview": { "distribution": "internal" },
    "production": {
      "autoIncrement": true,
      "android": { "buildType": "app-bundle" },
      "env": { "APP_ENV": "production" }
    },
    "preview-apk": {
      "android": { "buildType": "apk" },
      "env": { "APP_ENV": "production" }
    }
  },
  "submit": { "production": {} }
}
